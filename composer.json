{
    "name": "webignition/web-resource-model",
    "description": "Model of a web-based resource",
    "keywords": ["modem", "web", "psr-7"],
    "homepage": "https://github.com/webignition/web-resource-model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\WebResource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\Tests\\WebResource\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.2",
        "psr/http-message": "^1",
        "webignition/web-resource-interfaces":">=2.2,<3",
        "webignition/internet-media-type-interface":"^0.1",
        "webignition/internet-media-type":"^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3",
        "mockery/mockery": "^1",
        "webignition/web-resource-testing-tools":"^0.4"
    },
    "minimum-stability":"stable",
    "prefer-stable":true
}